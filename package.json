    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^1.5.2"
  },
  "dependencies": {
    "@headlessui/react": "^2.0.3",
    "@hookform/resolvers": "^3.3.4",
    "axios": "^1.6.8",
    "openapi-typescript": "^6.7.5",
    "react": "^18.2.0",
